createButton("üéØ TP em Cima da Base (com Brainrot)", function()
    local hasBrainrot = false

    -- Verifica se o jogador est√° segurando ou tem o item na mochila
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool:IsA("Tool") and tool.Name:lower():find("brainrot") then
            hasBrainrot = true
            break
        end
    end

    local charTool = Character:FindFirstChildOfClass("Tool")
    if charTool and charTool.Name:lower():find("brainrot") then
        hasBrainrot = true
    end

    if not hasBrainrot then
        local warnLabel = Instance.new("TextLabel", ScreenGui)
        warnLabel.Size = UDim2.new(0, 300, 0, 50)
        warnLabel.Position = UDim2.new(0.5, -150, 0, 0)
        warnLabel.Text = "‚ùå Voc√™ precisa estar com o Brainrot!"
        warnLabel.TextColor3 = Color3.new(1, 0.5, 0.5)
        warnLabel.BackgroundTransparency = 0.3
        warnLabel.BackgroundColor3 = Color3.fromRGB(120, 0, 0)
        warnLabel.TextScaled = true
        task.delay(3, function() warnLabel:Destroy() end)
        return
    end

    -- Procura inimigos e base
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local enemyHRP = player.Character.HumanoidRootPart

            local foundBase = nil
            for _, obj in pairs(workspace:GetChildren()) do
                if obj:IsA("BasePart") and obj.Name:lower():find("base") then
                    if (obj.Position - enemyHRP.Position).Magnitude < 100 then
                        foundBase = obj
                        break
                    end
                end
            end

            if foundBase then
                Character:PivotTo(foundBase.CFrame + Vector3.new(0, 10, 0))
                return
            else
                Character:PivotTo(enemyHRP.CFrame + Vector3.new(0, 30, 0))
                return
            end
        end
    end

    local n = Instance.new("TextLabel", ScreenGui)
    n.Size = UDim2.new(0, 300, 0, 50)
    n.Position = UDim2.new(0.5, -150, 0, 0)
    n.Text = "‚ùå Nenhum inimigo encontrado."
    n.TextColor3 = Color3.new(1, 0.2, 0.2)
    n.BackgroundTransparency = 0.4
    n.BackgroundColor3 = Color3.fromRGB(80, 0, 0)
    n.TextScaled = true
    task.delay(3, function() n:Destroy() end)
end)
-- üìå Bot√£o flutuante para abrir/fechar menu principal (com imagem do Minato)
local openButton = Instance.new("ImageButton", ScreenGui)
openButton.Name = "OpenMinatoMenu"
openButton.Size = UDim2.new(0, 60, 0, 60)
openButton.Position = UDim2.new(0, 10, 1, -70)
openButton.BackgroundTransparency = 0.2
openButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
openButton.BorderSizePixel = 0
openButton.Image = "rbxassetid://13068414756" -- ‚¨ÖÔ∏è ID do Minato (pode trocar)

local label = Instance.new("TextLabel", openButton)
label.Size = UDim2.new(1, 0, 0, 20)
label.Position = UDim2.new(0, 0, 1, 0)
label.Text = "Minato Hub"
label.TextColor3 = Color3.new(1, 1, 0)
label.TextScaled = true
label.BackgroundTransparency = 1
label.Font = Enum.Font.SourceSansBold

-- Toggle da GUI
openButton.MouseButton1Click:Connect(function()
¬† ¬† Frame.Visible = not Frame.Visible
end)
-- Servi√ßos
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

-- GUI principal
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
ScreenGui.Name = "MinatoHub"

-- Frame principal (menu)
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 300, 0, 400)
Frame.Position = UDim2.new(0.5, -150, 0.5, -200)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BorderSizePixel = 0
Frame.Visible = false

-- Cabe√ßalho do menu com imagem e nome
local header = Instance.new("Frame", Frame)
header.Size = UDim2.new(1, 0, 0, 100)
header.Position = UDim2.new(0, 0, 0, 0)
header.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
header.BorderSizePixel = 0

local minatoImage = Instance.new("ImageLabel", header)
minatoImage.Size = UDim2.new(0, 80, 0, 80)
minatoImage.Position = UDim2.new(0, 10, 0.5, -40)
minatoImage.Image = "rbxassetid://13068414756"
minatoImage.BackgroundTransparency = 1

local nameLabel = Instance.new("TextLabel", header)
nameLabel.Size = UDim2.new(1, -100, 0, 30)
nameLabel.Position = UDim2.new(0, 100, 0, 10)
nameLabel.Text = "üë§ " .. LocalPlayer.DisplayName
nameLabel.Font = Enum.Font.SourceSansBold
nameLabel.TextColor3 = Color3.new(1, 1, 1)
nameLabel.TextScaled = true
nameLabel.BackgroundTransparency = 1
nameLabel.TextXAlignment = Enum.TextXAlignment.Left

local titleLabel = Instance.new("TextLabel", header)
titleLabel.Size = UDim2.new(1, -100, 0, 30)
titleLabel.Position = UDim2.new(0, 100, 0, 50)
titleLabel.Text = "üåÄ Minato Hub"
titleLabel.Font = Enum.Font.SourceSansBold
titleLabel.TextColor3 = Color3.new(1, 1, 0)
titleLabel.TextScaled = true
titleLabel.BackgroundTransparency = 1
titleLabel.TextXAlignment = Enum.TextXAlignment.Left

-- Vari√°vel de controle de layout dos bot√µes
local yOffset = 100 -- Come√ßa abaixo do header

-- Fun√ß√£o para criar se√ß√µes
local function createSection(title)
	local section = Instance.new("TextLabel", Frame)
	section.Size = UDim2.new(1, -20, 0, 30)
	section.Position = UDim2.new(0, 10, 0, yOffset)
	section.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	section.TextColor3 = Color3.new(1, 1, 0)
	section.Font = Enum.Font.SourceSansBold
	section.Text = title
	section.TextScaled = true
	section.BorderSizePixel = 0
	yOffset = yOffset + 35
end

-- Fun√ß√£o para criar bot√µes no menu
local function createButton(name, callback)
	local btn = Instance.new("TextButton", Frame)
	btn.Size = UDim2.new(1, -20, 0, 50)
	btn.Position = UDim2.new(0, 10, 0, yOffset)
	btn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.Text = name
	btn.TextScaled = true
	btn.Font = Enum.Font.SourceSansBold
	btn.MouseButton1Click:Connect(callback)
	yOffset = yOffset + 55
end

-- === Se√ß√£o: Brainrot Tools ===
createSection("üß† Brainrot Tools")

-- Bot√£o: TP em Cima da Base (com Brainrot)
createButton("üéØ TP em Cima da Base (com Brainrot)", function()
	local hasBrainrot = false

	for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
		if tool:IsA("Tool") and tool.Name:lower():find("brainrot") then
			hasBrainrot = true
			break
		end
	end

	local charTool = Character:FindFirstChildOfClass("Tool")
	if charTool and charTool.Name:lower():find("brainrot") then
		hasBrainrot = true
	end

	if not hasBrainrot then
		local warnLabel = Instance.new("TextLabel", ScreenGui)
		warnLabel.Size = UDim2.new(0, 300, 0, 50)
		warnLabel.Position = UDim2.new(0.5, -150, 0, 0)
		warnLabel.Text = "‚ùå Voc√™ precisa estar com o Brainrot!"
		warnLabel.TextColor3 = Color3.new(1, 0.5, 0.5)
		warnLabel.BackgroundTransparency = 0.3
		warnLabel.BackgroundColor3 = Color3.fromRGB(120, 0, 0)
		warnLabel.TextScaled = true
		task.delay(3, function() warnLabel:Destroy() end)
		return
	end

	for _, player in pairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local enemyHRP = player.Character.HumanoidRootPart
			local foundBase = nil

			for _, obj in pairs(workspace:GetChildren()) do
				if obj:IsA("BasePart") and obj.Name:lower():find("base") then
					if (obj.Position - enemyHRP.Position).Magnitude < 100 then
						foundBase = obj
						break
					end
				end
			end

			if foundBase then
				Character:PivotTo(foundBase.CFrame + Vector3.new(0, 10, 0))
				return
			else
				Character:PivotTo(enemyHRP.CFrame + Vector3.new(0, 30, 0))
				return
			end
		end
	end

	local n = Instance.new("TextLabel", ScreenGui)
	n.Size = UDim2.new(0, 300, 0, 50)
	n.Position = UDim2.new(0.5, -150, 0, 0)
	n.Text = "‚ùå Nenhum inimigo encontrado."
	n.TextColor3 = Color3.new(1, 0.2, 0.2)
	n.BackgroundTransparency = 0.4
	n.BackgroundColor3 = Color3.fromRGB(80, 0, 0)
	n.TextScaled = true
	task.delay(3, function() n:Destroy() end)
end)

-- === Bot√£o flutuante para abrir/fechar o menu ===
local openButton = Instance.new("ImageButton", ScreenGui)
openButton.Name = "OpenMinatoMenu"
openButton.Size = UDim2.new(0, 60, 0, 60)
openButton.Position = UDim2.new(0, 10, 1, -70)
openButton.BackgroundTransparency = 0.2
openButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
openButton.BorderSizePixel = 0
openButton.Image = "rbxassetid://13068414756"

local label = Instance.new("TextLabel", openButton)
label.Size = UDim2.new(1, 0, 0, 20)
label.Position = UDim2.new(0, 0, 1, 0)
label.Text = "Minato Hub"
label.TextColor3 = Color3.new(1, 1, 0)
label.TextScaled = true
label.BackgroundTransparency = 1
label.Font = Enum.Font.SourceSansBold

openButton.MouseButton1Click:Connect(function()
	Frame.Visible = not Frame.Visible
end)










































































































































































































































